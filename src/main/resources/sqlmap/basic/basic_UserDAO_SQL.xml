    <insert id="insertUser" parameterType="hae.basic.vo.UserVO">
        <![CDATA[
            INSERT /* QueryID=hae.basic.service.dao.UserDAO.insertUser */ 
              INTO USERIFTB
                   (USER_ID,
                   USER_PWD)
            VALUES (#{userID},
                   #{userPW})
        ]]>
    </insert>
    
    
