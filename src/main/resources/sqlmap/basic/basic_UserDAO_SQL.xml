<mapper namespace="hae.basic.service.dao.UserMDAO">

    <resultMap id="sample" type="hae.basic.vo.UserVO">
        <result property="userID" column="USER_ID"/>
        <result property="userPW" column="USER_PWD"/>
    </resultMap>
    
    <!--
        Query Name : insert_id
        Description : INSERT 설명을 기술합니다.
